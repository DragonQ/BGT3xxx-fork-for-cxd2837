	@files='tda18271.ko tuner-types.ko tuner-simple.ko'; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/common/tuners/$$i ]; then echo -n "$$i "; rm $(DESTDIR)$(KDIR26)/common/tuners/$$i; fi; done; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/common/tuners/$$i.gz ]; then echo -n "$$i.gz "; rm $(DESTDIR)$(KDIR26)/common/tuners/$$i.gz; fi; done; echo;

