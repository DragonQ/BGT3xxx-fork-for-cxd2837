media-install::
	@echo -e "\nRemoving obsolete files from $(KDIR26)/dvb/frontends:"
	@files='tda18271.ko xc5000.ko mt2060.ko mt2131.ko mt2266.ko qt1010.ko tda827x.ko'; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/dvb/frontends/$$i ]; then echo -n "$$i "; rm $(DESTDIR)$(KDIR26)/dvb/frontends/$$i; fi; done; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/dvb/frontends/$$i.gz ]; then echo -n "$$i.gz "; rm $(DESTDIR)$(KDIR26)/dvb/frontends/$$i.gz; fi; done; echo;

	@echo -e "\nRemoving obsolete files from $(KDIR26)/dvb/cinergyT2:"
	@files='cinergyT2.ko'; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/dvb/cinergyT2/$$i ]; then echo -n "$$i "; rm $(DESTDIR)$(KDIR26)/dvb/cinergyT2/$$i; fi; done; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/dvb/cinergyT2/$$i.gz ]; then echo -n "$$i.gz "; rm $(DESTDIR)$(KDIR26)/dvb/cinergyT2/$$i.gz; fi; done; echo;

	@echo -e "\nRemoving obsolete files from $(KDIR26)/video:"
	@files='tda9887.ko video-buf.ko mt20xx.ko video-buf-dvb.ko tuner-xc2028.ko tuner-simple.ko tuner-types.ko compat-ioctl32.ko tea5767.ko tea5761.ko tda8290.ko'; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/video/$$i ]; then echo -n "$$i "; rm $(DESTDIR)$(KDIR26)/video/$$i; fi; done; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/video/$$i.gz ]; then echo -n "$$i.gz "; rm $(DESTDIR)$(KDIR26)/video/$$i.gz; fi; done; echo;

	@if [ -d /lib/modules/$(KERNELRELEASE)/ubuntu/media ]; then printf "\nHmm... distro kernel with a non-standard place for module backports detected.\nPlease always prefer to use vanilla upstream kernel with V4L/DVB\nI'll try to remove old/obsolete LUM files from /lib/modules/$(KERNELRELEASE)/ubuntu/media:\n"; files=' cxd2843.ko stv090x.ko cxd2817.ko s5h1411.ko tda18272.ko tda10048.ko dvb_dummy_fe.ko a8290.ko cxd2820r.ko cxd2850.ko lnbp21.ko cxd2861.ko dvb-pll.ko stv6110x.ko videobuf2-memops.ko videobuf2-vmalloc.ko videobuf-dma-contig.ko videobuf-dvb.ko videobuf2-dma-sg.ko v4l2-mem2mem.ko videobuf-vmalloc.ko videobuf2-dvb.ko videobuf-core.ko videobuf-dma-sg.ko v4l2-common.ko videodev.ko videobuf2-core.ko v4l2-dv-timings.ko tuner.ko videobuf2-dma-contig.ko ir-common.ko dvb-core.ko saa7231_core.ko saa7231_drv.ko tda18271.ko tuner-types.ko tuner-simple.ko tda18271.ko xc5000.ko mt2060.ko mt2131.ko mt2266.ko qt1010.ko tda827x.ko cinergyT2.ko tda9887.ko video-buf.ko mt20xx.ko video-buf-dvb.ko tuner-xc2028.ko tuner-simple.ko tuner-types.ko compat-ioctl32.ko tea5767.ko tea5761.ko tda8290.ko'; for i in $$files;do find "/lib/modules/$(KERNELRELEASE)/ubuntu/media" -name "$$i" -exec echo '{}' ';' ; find "/lib/modules/$(KERNELRELEASE)/ubuntu/media" -name "$$i" -exec rm '{}' ';' ; done; fi
	@echo "Installing kernel modules under $(DESTDIR)$(KDIR26)/:"
	@n=0;for i in cxd2843.ko stv090x.ko cxd2817.ko s5h1411.ko tda18272.ko tda10048.ko dvb_dummy_fe.ko a8290.ko cxd2820r.ko cxd2850.ko lnbp21.ko cxd2861.ko dvb-pll.ko stv6110x.ko;do if [ -e "$$i" ]; then if [ $$n -eq 0 ]; then echo -n "	dvb/frontends/: "; install -d $(DESTDIR)$(KDIR26)/dvb/frontends; fi; n=$$(($$n+1)); if [  $$n -eq 4 ]; then echo; echo -n "		"; n=1; fi; echo -n "$$i "; install -m 644 -c $$i $(DESTDIR)$(KDIR26)/dvb/frontends; fi; done; if [  $$n -ne 0 ]; then echo; strip --strip-debug $(DESTDIR)$(KDIR26)/dvb/frontends/*.ko; fi;

	@n=0;for i in videobuf2-memops.ko videobuf2-vmalloc.ko videobuf-dma-contig.ko videobuf-dvb.ko videobuf2-dma-sg.ko v4l2-mem2mem.ko videobuf-vmalloc.ko videobuf2-dvb.ko videobuf-core.ko videobuf-dma-sg.ko v4l2-common.ko videodev.ko videobuf2-core.ko v4l2-dv-timings.ko tuner.ko videobuf2-dma-contig.ko;do if [ -e "$$i" ]; then if [ $$n -eq 0 ]; then echo -n "	video/: "; install -d $(DESTDIR)$(KDIR26)/video; fi; n=$$(($$n+1)); if [  $$n -eq 4 ]; then echo; echo -n "		"; n=1; fi; echo -n "$$i "; install -m 644 -c $$i $(DESTDIR)$(KDIR26)/video; fi; done; if [  $$n -ne 0 ]; then echo; strip --strip-debug $(DESTDIR)$(KDIR26)/video/*.ko; fi;

	@n=0;for i in ir-common.ko;do if [ -e "$$i" ]; then if [ $$n -eq 0 ]; then echo -n "	common/: "; install -d $(DESTDIR)$(KDIR26)/common; fi; n=$$(($$n+1)); if [  $$n -eq 4 ]; then echo; echo -n "		"; n=1; fi; echo -n "$$i "; install -m 644 -c $$i $(DESTDIR)$(KDIR26)/common; fi; done; if [  $$n -ne 0 ]; then echo; strip --strip-debug $(DESTDIR)$(KDIR26)/common/*.ko; fi;

	@n=0;for i in dvb-core.ko;do if [ -e "$$i" ]; then if [ $$n -eq 0 ]; then echo -n "	dvb/dvb-core/: "; install -d $(DESTDIR)$(KDIR26)/dvb/dvb-core; fi; n=$$(($$n+1)); if [  $$n -eq 4 ]; then echo; echo -n "		"; n=1; fi; echo -n "$$i "; install -m 644 -c $$i $(DESTDIR)$(KDIR26)/dvb/dvb-core; fi; done; if [  $$n -ne 0 ]; then echo; strip --strip-debug $(DESTDIR)$(KDIR26)/dvb/dvb-core/*.ko; fi;

	@n=0;for i in saa7231_core.ko saa7231_drv.ko;do if [ -e "$$i" ]; then if [ $$n -eq 0 ]; then echo -n "	saa7231/: "; install -d $(DESTDIR)$(KDIR26)/saa7231; fi; n=$$(($$n+1)); if [  $$n -eq 4 ]; then echo; echo -n "		"; n=1; fi; echo -n "$$i "; install -m 644 -c $$i $(DESTDIR)$(KDIR26)/saa7231; fi; done; if [  $$n -ne 0 ]; then echo; strip --strip-debug $(DESTDIR)$(KDIR26)/saa7231/*.ko; fi;

	@n=0;for i in tda18271.ko tuner-types.ko tuner-simple.ko;do if [ -e "$$i" ]; then if [ $$n -eq 0 ]; then echo -n "	common/tuners/: "; install -d $(DESTDIR)$(KDIR26)/common/tuners; fi; n=$$(($$n+1)); if [  $$n -eq 4 ]; then echo; echo -n "		"; n=1; fi; echo -n "$$i "; install -m 644 -c $$i $(DESTDIR)$(KDIR26)/common/tuners; fi; done; if [  $$n -ne 0 ]; then echo; strip --strip-debug $(DESTDIR)$(KDIR26)/common/tuners/*.ko; fi;

	
	/sbin/depmod -a $(KERNELRELEASE) $(if $(DESTDIR),-b $(DESTDIR))

media-rminstall::
	@echo -e "\nRemoving obsolete files from $(KDIR26)/dvb/frontends:"
	@files='tda18271.ko xc5000.ko mt2060.ko mt2131.ko mt2266.ko qt1010.ko tda827x.ko'; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/dvb/frontends/$$i ]; then echo -n "$$i "; rm $(DESTDIR)$(KDIR26)/dvb/frontends/$$i; fi; done; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/dvb/frontends/$$i.gz ]; then echo -n "$$i.gz "; rm $(DESTDIR)$(KDIR26)/dvb/frontends/$$i.gz; fi; done; echo;

	@echo -e "\nRemoving obsolete files from $(KDIR26)/dvb/cinergyT2:"
	@files='cinergyT2.ko'; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/dvb/cinergyT2/$$i ]; then echo -n "$$i "; rm $(DESTDIR)$(KDIR26)/dvb/cinergyT2/$$i; fi; done; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/dvb/cinergyT2/$$i.gz ]; then echo -n "$$i.gz "; rm $(DESTDIR)$(KDIR26)/dvb/cinergyT2/$$i.gz; fi; done; echo;

	@echo -e "\nRemoving obsolete files from $(KDIR26)/video:"
	@files='tda9887.ko video-buf.ko mt20xx.ko video-buf-dvb.ko tuner-xc2028.ko tuner-simple.ko tuner-types.ko compat-ioctl32.ko tea5767.ko tea5761.ko tda8290.ko'; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/video/$$i ]; then echo -n "$$i "; rm $(DESTDIR)$(KDIR26)/video/$$i; fi; done; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/video/$$i.gz ]; then echo -n "$$i.gz "; rm $(DESTDIR)$(KDIR26)/video/$$i.gz; fi; done; echo;

	@if [ -d /lib/modules/$(KERNELRELEASE)/ubuntu/media ]; then printf "\nHmm... distro kernel with a non-standard place for module backports detected.\nPlease always prefer to use vanilla upstream kernel with V4L/DVB\nI'll try to remove old/obsolete LUM files from /lib/modules/$(KERNELRELEASE)/ubuntu/media:\n"; files=' cxd2843.ko stv090x.ko cxd2817.ko s5h1411.ko tda18272.ko tda10048.ko dvb_dummy_fe.ko a8290.ko cxd2820r.ko cxd2850.ko lnbp21.ko cxd2861.ko dvb-pll.ko stv6110x.ko videobuf2-memops.ko videobuf2-vmalloc.ko videobuf-dma-contig.ko videobuf-dvb.ko videobuf2-dma-sg.ko v4l2-mem2mem.ko videobuf-vmalloc.ko videobuf2-dvb.ko videobuf-core.ko videobuf-dma-sg.ko v4l2-common.ko videodev.ko videobuf2-core.ko v4l2-dv-timings.ko tuner.ko videobuf2-dma-contig.ko ir-common.ko dvb-core.ko saa7231_core.ko saa7231_drv.ko tda18271.ko tuner-types.ko tuner-simple.ko tda18271.ko xc5000.ko mt2060.ko mt2131.ko mt2266.ko qt1010.ko tda827x.ko cinergyT2.ko tda9887.ko video-buf.ko mt20xx.ko video-buf-dvb.ko tuner-xc2028.ko tuner-simple.ko tuner-types.ko compat-ioctl32.ko tea5767.ko tea5761.ko tda8290.ko'; for i in $$files;do find "/lib/modules/$(KERNELRELEASE)/ubuntu/media" -name "$$i" -exec echo '{}' ';' ; find "/lib/modules/$(KERNELRELEASE)/ubuntu/media" -name "$$i" -exec rm '{}' ';' ; done; fi
	@echo -e "\nRemoving old $(KDIR26)/dvb/frontends files:"
	@files='cxd2843.ko stv090x.ko cxd2817.ko s5h1411.ko tda18272.ko tda10048.ko dvb_dummy_fe.ko a8290.ko cxd2820r.ko cxd2850.ko lnbp21.ko cxd2861.ko dvb-pll.ko stv6110x.ko'; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/dvb/frontends/$$i ]; then echo -n "$$i "; rm $(DESTDIR)$(KDIR26)/dvb/frontends/$$i; fi; done; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/dvb/frontends/$$i.gz ]; then echo -n "$$i.gz "; rm $(DESTDIR)$(KDIR26)/dvb/frontends/$$i.gz; fi; done; echo;

	@echo -e "\nRemoving old $(KDIR26)/video files:"
	@files='videobuf2-memops.ko videobuf2-vmalloc.ko videobuf-dma-contig.ko videobuf-dvb.ko videobuf2-dma-sg.ko v4l2-mem2mem.ko videobuf-vmalloc.ko videobuf2-dvb.ko videobuf-core.ko videobuf-dma-sg.ko v4l2-common.ko videodev.ko videobuf2-core.ko v4l2-dv-timings.ko tuner.ko videobuf2-dma-contig.ko'; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/video/$$i ]; then echo -n "$$i "; rm $(DESTDIR)$(KDIR26)/video/$$i; fi; done; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/video/$$i.gz ]; then echo -n "$$i.gz "; rm $(DESTDIR)$(KDIR26)/video/$$i.gz; fi; done; echo;

	@echo -e "\nRemoving old $(KDIR26)/common files:"
	@files='ir-common.ko'; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/common/$$i ]; then echo -n "$$i "; rm $(DESTDIR)$(KDIR26)/common/$$i; fi; done; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/common/$$i.gz ]; then echo -n "$$i.gz "; rm $(DESTDIR)$(KDIR26)/common/$$i.gz; fi; done; echo;

	@echo -e "\nRemoving old $(KDIR26)/dvb/dvb-core files:"
	@files='dvb-core.ko'; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/dvb/dvb-core/$$i ]; then echo -n "$$i "; rm $(DESTDIR)$(KDIR26)/dvb/dvb-core/$$i; fi; done; for i in $$files;do if [ -e $(DESTDIR)$(KDIR26)/dvb/dvb-core/$$i.gz ]; then echo -n "$$i.gz "; rm $(DESTDIR)$(KDIR26)/dvb/dvb-core/$$i.gz; fi; done; echo;

